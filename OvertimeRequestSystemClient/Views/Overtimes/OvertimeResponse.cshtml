@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}
<!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Dashboard</h1>
</div>


<div class="card mb-4">
    <div class="card-body">
     
        <table style="width:100%" class="table dt-responsive nowrap mt-2" id="tableOvertime">
            <thead class="thead-dark">
                <tr>
                    <th class="tbtitle text-center" scope="col" colspan="8">Employee List</th>
                </tr>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Overtime Id</th>
                    <th scope="col">Date</th>
                    <th scope="col">Status By Manager</th>
                    <th scope="col">Status By Finance</th>
                    <th scope="col">NIP</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/Overtime/OvertimeResponse.js"></script>
}